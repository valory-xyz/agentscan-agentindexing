# Required: RPC URLs for supported networks
PONDER_RPC_URL_1= # Ethereum
PONDER_RPC_URL_100= # Gnosis Chain
PONDER_RPC_URL_8453= # Base

# Optional: Main database URL (defaults to SQLite if not provided)
DATABASE_URL="postgresql://postgres@localhost:5432/agentscan"

# Optional: ABI database URL
# If not provided, ABI storage and embeddings will be disabled
ABI_DATABASE_URL="postgresql://postgres@localhost:5432/agentscan_abi"

# Optional: Redis URL
# If not provided, Redis caching will be disabled
REDIS_URL="redis://localhost:6379"

# Optional: OpenAI API key
# Required only if ABI_DATABASE_URL is set (for embeddings generation)
OPENAI_API_KEY="your-openai-api-key"

IS_LOCAL=true




